            <owl:onProperty>
              <owl:ObjectProperty rdf:about="http://sadl.org/DESIGN.sadl#requirementType"/>
            </owl:onProperty>
          </owl:Restriction>
        </rdfs:subClassOf>
      </rdf:Description>
    </rdfs:subClassOf>
  </owl:Class>
