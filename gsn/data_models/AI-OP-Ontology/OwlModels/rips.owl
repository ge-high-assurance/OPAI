  </sys:SYSTEM>
  <sys:SYSTEM rdf:about="http://sadl.org/rips.sadl#Battery-Health-Monitor">
    <sys:function>
      <sys:FUNCTION rdf:about="http://sadl.org/rips.sadl#Maintenance-indication">
        <provs:description>The BHM will indicate that maintenance is required when the state of health (SOH) of the battery is 70% or less</provs:description>
        <provs:identifier>Maintenance-indication</provs:identifier>
        <rdfs:comment xml:lang="en">Provide 'maintenance required' indication</rdfs:comment>
      </sys:FUNCTION>
    </sys:function>
    <sys:function>
      <sys:FUNCTION rdf:about="http://sadl.org/rips.sadl#Charge-command">
        <provs:description>The BHM will estimate the state of charge of the battery and command a full recharge if the state of charge (SOC) of the battery has reached 30% or less of the nominal charge capacity</provs:description>
        <provs:identifier>Charge-command</provs:identifier>
        <rdfs:comment xml:lang="en">Provide 'full recharge' command</rdfs:comment>
      </sys:FUNCTION>
    </sys:function>
