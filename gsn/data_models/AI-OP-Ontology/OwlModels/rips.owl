  <sys:SYSTEM rdf:about="http://sadl.org/rips.sadl#Recorder-independent-power-supply">
    <sys:function>
      <sys:FUNCTION rdf:about="http://sadl.org/rips.sadl#Backup-power">
        <provs:description>Provide backup power to the Flight Data Recorder</provs:description>
        <provs:identifier>Backup-power</provs:identifier>
        <rdfs:comment xml:lang="en">Provide backup power to the Flight Data Recorder</rdfs:comment>
      </sys:FUNCTION>
    </sys:function>
